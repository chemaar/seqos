<?xml version="1.0" encoding="utf-8"?>    
<!DOCTYPE urlrewrite   PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
"webapps/seqos/WEB-INF/urlrewrite2.6.dtd">
<urlrewrite>


<rule>
    <from>^/ontology[(\s)|(\/)]*$</from>
    <to type="redirect">/datasets/seqos/seqos-onto.ttl</to>
</rule>
        

<rule>
    <from>^/seqos[(\s)|(\/)]*$</from>
    <to type="redirect">/seqos/index/ds</to>
</rule>



<rule>
    <from>^/index[(\s)|(\/)]*$</from>
    <to type="redirect">/seqos/index/ds</to>
</rule>

<rule>
    <from>^/component[(\s)|(\/)]*$</from>
    <to type="redirect">/seqos/component/ds</to>
</rule>

<rule>
    <from>^/indicator[(\s)|(\/)]*$</from>
    <to type="redirect">/seqos/indicator/ds</to>
</rule>

<rule>
    <from>^/slice[(\s)|(\/)]*$</from>
    <to type="redirect">/seqos/slice/ds</to>
</rule>

<rule>
    <from>^/dsd[(\s)|(\/)]*$</from>
    <to type="redirect">/seqos/dsd/ds</to>
</rule>

<rule>
    <from>^/dataset[(\s)|(\/)]*$</from>
    <to type="redirect">/seqos/dataset/ds</to>
</rule>


<rule>
    <from>^/observation[(\s)|(\/)]*$</from>
    <to type="redirect">/seqos/observation/ds</to>
</rule>

<rule>
    <from>^/observation/external[(\s)|(\/)]*$</from>
    <to type="redirect">/seqos/observation/external/ds</to>
</rule>


<rule>
    <from>^/indicator/external[(\s)|(\/)]*$</from>
    <to type="redirect">/seqos/indicator/external/ds</to>
</rule>


<rule>
    <from>^/area[(\s)|(\/)]*$</from>
    <to type="redirect">/seqos/area/ds</to>
</rule>

<rule>
    <from>^/area/region[(\s)|(\/)]*$</from>
    <to type="redirect">/seqos/area/region/ds</to>
</rule>

<rule>
    <from>^/area/country[(\s)|(\/)]*$</from>
    <to type="redirect">/seqos/area/country/ds</to>
</rule>
        
<rule>
    <from>^/organization[(\s)|(\/)]*$</from>
    <to type="redirect">/seqos/organization/ds</to>
</rule>



</urlrewrite>
